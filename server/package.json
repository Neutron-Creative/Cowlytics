{
  "name": "cowlytics-server",
  "version": "0.1.0",
  "description": "The server for the Cowlytics open-source software.",
  "main": "index.js",
  "scripts": {
    "copy": "(copyfiles -f \"src/sql/*.sql\" dist/sql) && (copyfiles -f src/crypto/*.* dist/crypto/)",
    "start": "node dist/index.js",
    "build": "tsc && npm run copy",
    "dev": "ts-node src/App.ts",
    "postversion": "auto-changelog --output ../CHANGELOG.md && git add ../CHANGELOG.md && cd ../client && npm version patch && cd ../proxy && npm version patch",
    "test": "jest",
    "test-debug": "jest --runInBand --detectOpenHandles"
  },
  "author": "Neutron Creative Inc.",
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "axios": "^0.21.0",
    "bcrypt": "^5.0.0",
    "fastify": "^3.8.0",
    "fastify-cors": "^4.1.0",
    "fastify-favicon": "^3.0.0",
    "fastify-rate-limit": "^4.0.3",
    "http-status-codes": "^2.1.4",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@types/bcrypt": "^3.0.0",
    "@types/jest": "^26.0.15",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.14.8",
    "@types/pg": "^7.14.7",
    "@types/stripe": "^7.13.25",
    "@types/validator": "^13.1.0",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "auto-changelog": "^2.2.1",
    "babel-jest": "^26.6.3",
    "copyfiles": "^2.4.0",
    "eslint": "^7.13.0",
    "jest": "^26.6.3",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.1.2"
  }
}
